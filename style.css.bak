        a.tooltips {
  position: absolute;
  display: inline;
}
a.tooltips span {
  position: absolute;
  width:140px;
  color: #C5CEAB;
  background: #DC4D3A;
  height: 30px;
  line-height: 30px;
  text-align: center;
  visibility: hidden;
  border-radius: 6px;
  box-shadow: -3px 3px 2px #907D96;
}
a.tooltips span:after {
  content: '';
  position: absolute;
  top: 100%;
  left: 50%;
  margin-left: -8px;
  width: 0; height: 0;
  border-top: 8px solid #DC4D3A;
  border-right: 8px solid transparent;
  border-left: 8px solid transparent;
}
a:hover.tooltips span {
  visibility: visible;
  opacity: 0.8;
  bottom: 30px;
  left: 50%;
  margin-left: -70px;
  margin-bottom: 115px;
  z-index: 999;
}
